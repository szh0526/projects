{
  "name": "webpack-sample-project",
  "version": "1.0.0",
  "description": "Sample webpack project",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "webpack --progress --colors --config webpack.config.js",
    "builddll": "webpack --config webpack.dll.config.js",
    "prestart": "npm run builddll && npm run build",
    "start": "webpack-dev-server --display-error-details",
    "sourcemap": "webpack-dev-server -d --inline --host localhost --port 8089 --display-error-details --devtool eval-source-map --progress --colors --content-base dist",
    "server": "http-server -p 10000 dist/",
    "buildserver": "npm run build && npm run server"
  },
  "author": "Super 'Kakarot' Saiyan",
  "keywords": [
    "webpack"
  ],
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "connect-rest": "^1.9.5",
    "css-loader": "^0.26.1",
    "del": "^2.2.2",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-connect": "^5.0.0",
    "gulp-less": "^3.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.7",
    "html-webpack-plugin": "^2.24.1",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "jsx-loader": "^0.13.2",
    "moment": "^2.17.1",
    "postcss-loader": "^1.2.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-transform-hmr": "^1.0.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/szh0526/reactweb.git"
  }
}
