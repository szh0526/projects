{
  "name": "webpack-sample-project",
  "version": "1.0.0",
  "description": "Sample webpack project",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "cleanbin": "rimraf bin/*",
    "buildDev": "webpack --progress --colors --config webpack.config.js",
    "buildDll": "webpack --config webpack.dll.config.js",
    "prestart": "SET NODE_ENV=development&& npm run buildDll && npm run buildDev",
    "start": "webpack-dev-server --display-error-details",
    "devserver": "webpack-dev-server --debug --hot --inline --host localhost --port 8089 --display-error-details --devtool eval-source-map --progress --colors --content-base dist",
    "server": "http-server -p 10000 dist/",
    "buildserver": "npm run build && npm run server",
    "startPrd": "SET NODE_ENV=production&& npm run buildDll && npm run buildPrd",
    "buildPrd": "webpack --progress --colors --config webpack.production.config.js",
    "mockserver": "json-server mock/db.json --routes mock/routes.json --port 9999"
  },
  "author": "Super 'Kakarot' Saiyan",
  "keywords": [
    "webpack"
  ],
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.16.0",
    "compression-webpack-plugin": "^0.3.2",
    "connect-rest": "^1.9.5",
    "css-loader": "^0.26.1",
    "del": "^2.2.2",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-connect": "^5.0.0",
    "gulp-less": "^3.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.7",
    "gulp-webserver": "^0.9.1",
    "html-webpack-plugin": "^2.24.1",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "jsx-loader": "^0.13.2",
    "less-loader": "^2.2.3",
    "moment": "^2.17.1",
    "postcss-loader": "^1.2.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.3",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.3",
    "webpack-spritesmith": "^0.3.1",
    "webpack-stream": "^3.2.0"
  },
  "dependencies": {
    "node-fetch": "^1.6.3",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/szh0526/reactweb.git"
  }
}
